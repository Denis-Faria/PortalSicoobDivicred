<script>
    function Seleciona(Id) {
        alert("--->" + Id);
        $("#EdicaoPermissoes").removeAttr("hidden");

        $('#ModalPermissoesFuncionario').removeClass();
        $('#ModalPermissoesFuncionario').addClass('modal is-active');
        void (0);
        
        $("#EdicaoPermissoes").empty();
        $.ajax(
            {
                type: 'Post',
                url: '/Parametros/ListaPermissoesFuncionario',
                dataType: 'html',
                cache: true,
                async: true,
                data: { "IdFuncionario": Id },
                success: function(data) {

                    $("#EdicaoPermissoes").append(data);

                }
            });

    }
</script>
<div>
    <ul>
        @for (var i = 0; i < Convert.ToInt32(TempData["TotalResultado"]); i++)
        {
            <li>
                <a href="Javascript:Seleciona('@TempData["Id" + i]')">
                    @TempData["Nome" + i]
                </a>
            </li>
        }
    </ul>
</div>