<script>
    function Seleciona(Id) {
        $("#EdicaoCertificacao").removeAttr("hidden");
        $("#SalvaCertificacao").removeAttr("disabled ");
        $("#ExcluirCertificacao").removeAttr("disabled ");
        $("#EdicaoCertificacao").empty();
        $.ajax(
            {
                type: 'Post',
                url: '/PainelColaborador/RecuperaCertificacao',
                dataType: 'html',
                cache: true,
                async: true,
                data: { "IdCertificacao": Id },
                success: function(data) {

                    $("#EdicaoCertificacao").append(data);

                }
            });

    }
</script>
<div>
    <ul>
        @for (var i = 0; i < Convert.ToInt32(TempData["TotalResultado"]); i++)
        {
            <li>
                <a href="Javascript:Seleciona('@TempData["Id" + i]')">
                    @TempData["Descricao" + i]
                </a>
            </li>
        }
    </ul>
</div>