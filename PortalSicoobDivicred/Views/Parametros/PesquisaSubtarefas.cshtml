<script>
    function SelecionaTarefa(id) {
        
     
        $("#EdicaoSubtarefas").removeAttr("hidden");
  
        $("#SalvaSubtarefa").removeAttr("disabled ");
        
        $("#EdicaoSubtarefas").empty();
        $.ajax(
            {
                type: 'Post',
                url: '/Parametros/RecuperaSubtarefa',
                dataType: 'html',
                cache: true,
                async: true,
                data: { "IdSubtarefa": id },
                success: function(data) {

                    $("#EdicaoSubtarefas").append(data);

                }
            });

    }
</script>
<div>
    <ul>
        @for (var i = 0; i < Convert.ToInt32(TempData["TotalResultado"]); i++)
        {
            <li>
                <a href="Javascript:SelecionaTarefa('@TempData["id" + i]')">
                    @TempData["Descricao" + i]
                </a>
            </li>
        }
    </ul>
</div>